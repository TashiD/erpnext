 "css": ".mytable {\n\twidth: 100%; \n}\n\n.mytable, .mytable th, .mytable td {\n\tborder: 1px solid #999;\n        line-height: 1;   \n}\n\n@media all {\n   div .printedby {\n       position: fixed;\n       bottom: 0;\n       text-align: right;\n       font-size: xx-small;\n       width: 100% !important;\n   }\n}\n\n.letter-head img {\n    margin-left: auto;\n    margin-right: auto;\n    display: block;\n}", 
 "custom_format": 1, 
