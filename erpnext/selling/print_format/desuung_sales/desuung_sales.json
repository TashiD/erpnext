 "doctype": "Print Format", 
 "font": "Default", 
 "html": "<div id=\"header-html\" class=\"hidden-pdf\">\n    {%- if letter_head and not no_letterhead -%}\n        <div class=\"letter-head\" style=\"width: 100%;\">\n            {{ letter_head }}\n        </div>\n    {%- endif -%}\n</div>\n    \n<div  style=\"text-align: center;\">\n    <h3>DESUUNG SALES</h3>\n    <h5><u>{{ doc.branch }}</u></h5>\n</div>\n\n<div style=\"width: 100%;\">\n    <div>\n        <div style=\"float: left;\"><b>Desuung Sales :</b> {{ doc.name }} </div>\n        <div style=\"float: right;\"><b>Date :</b> \n            {{ frappe.utils.formatdate(doc.transaction_date) }}\n        </div>\n    </div>\n    <div style=\"clear: both;\"></div>\n    {%- if doc.items[0].prevdoc_docname -%}\n    <div>\n        <b>Quotation Reference / Date :</b> \n        {{ doc.items[0].prevdoc_docname }}/{{frappe.utils.formatdate(frappe.db.get_value(\"Quotation\",      doc.items[0].prevdoc_docname, \"transaction_date\")) }}\n    </div>\n    {%- endif -%}\n    <br/>\n\n    <div>\n        <p><strong>Desuup Name:&nbsp</strong>{{ doc.customer_name }}</p>\n        <p><strong>Desuunp ID:&nbsp</strong>{{ doc.customer }}</p>\n        <p><strong>Journal No.:&nbsp</strong>{{ doc.journal_no }}</p>\n        <p><strong>Journal Date:&nbsp</strong>{{ doc.journal_date }}</p>\n        <!--\n        <p>Customer Address# {{ frappe.db.get_value(\"Customer\", doc.customer, \"location\")}}\n                <p>Mobile# \n                    {%- if doc.contact_mobile -%}\n                        {{ doc. contact_mobile }} \n                    {%- else -%}\n                        {{ frappe.db.get_value(\"Customer\", doc.customer, \n                                     \"mobile_no\") }} / {{ frappe.db.get_value(\"Customer\", doc.customer, \n                                      \"telephone_and_fax\") }}               \n                    {%- endif -%}               \n                <br/>&nbsp; \n                <p><b>Subject :</b> {{ doc.title }}</p>\n                <br/><p>Dear Sir/Madam,</p><br/>\n                {%- if doc.sales_order_header_text -%}\n                    <p>{{ doc.sales_order_header_text }} </p>\n                {%- endif -%} -->\n                <br/>       \n                \n        <!--<table cellpadding=\"0\" cellspacing=\"0\" class=\"mytable\" style=\"margin-top: 5px; text-align:center;font-size:small; width:650px\">-->\n        <table cellpadding=\"0\" cellspacing=\"0\" class=\"mytable\" style=\"margin-top: 5px; text-align:center;font-size:small\">\n            <!--  Table heading -->\n            <tr>\n                <th style=\"text-align: center;\">Sl.No</th>\n                <th style=\"text-align: center;\">Material code</th>\n                <th style=\"text-align: center;\">Material Name</th>\n                {%- if doc.naming_series == \"Timber Products\" -%}\n                <th style=\"text-align: center;\">Lot No.</th>\n                {%- endif -%}\n                <th style=\"text-align: center;\">Unit</th>\n                <th style=\"text-align: center;\">Qty</th>\n                <th style=\"text-align: right;\">Rate (Nu)</th>\n                <th style=\"text-align: right;\">Amount (Nu)</th>\n            </tr>\n                            \n            <!--  Table data -->\n            {%- for row in doc.items -%}\n            <tr>\n                <td>{{ row.idx }}</td>\n                <td>{{ row.item_code }}</td>\n                <td>{{ row.item_name }}</td>\n                {%- if doc.naming_series == \"Timber Products\" -%}\n                <td style=\"text-align: center\">{{ row.lot_number }}</td>\n                {%- endif -%}\n                <td>{{ row.stock_uom }}</td>\n                <td>{{ row.qty }}</td>\n                <td style=\"text-align: right;\">\n                    {{ frappe.format_value(row.rate, {\"fieldtype\":\"Currency\"}) }}\n                </td>\n                <td style=\"text-align: right;\">\n                    {{ frappe.format_value(row.amount, {\"fieldtype\":\"Currency\"}) }}\n                </td>\n            </tr>\n            {%- endfor -%}\n            <tr> \n                {%- if doc.naming_series == \"Timber Products\" -%}\n                    <td colspan=\"7\" style='text-align:right'>\n                {%- else -%}\n                    <td colspan=\"6\" style='text-align:right'>\n                {%- endif -%}\n                    <b>Total Amount</b></td>            \n                <td style='text-align:right'>{{ frappe.format_value(doc.total, \n                                {\"fieldtype\":\"Currency\"}) }}</td>\n            </tr>\n\n            <!--{%- if doc.discount_amount -%}\n            <tr>\n                {%- if doc.naming_series == \"Timber Products\" -%}\n                <td colspan=\"7\" style=\"text-align:right;\">\n                {%- else -%}\n                <td colspan=\"6\" style=\"text-align:right;\">\n                {%- endif -%}   \n                Additional Cost or Discount                     \n                </td>\n                <td style=\"text-align:right;\">\n                {{ frappe.format_value((doc.discount_or_cost_amount * -1) + doc.additional_cost, \n                             {\"fieldtype\":\"Currency\"}) }}\n                </td>\n            </tr>\n            -->\n            {%- endif -%}\n            {%- if doc.transportation_charges -%}\n            <tr>\n                    {%- if doc.naming_series == \"Timber Products\" -%}\n                <td colspan=\"7\" style=\"text-align:right;\">\n                {%- else -%}\n                <td colspan=\"6\" style=\"text-align:right;\">\n                {%- endif -%}   \n                <i>(Transportation Charges):</i></td>\n                <td style = \"text-align : right;\">{{ \n                    frappe.format_value(doc.transportation_charges, {\"fieldtype\":\"Currency\"}) }} \n                </td>\n            </tr>\n            {%- endif -%}\n            {%- if doc.loading_cost -%}\n            <tr>\n                {%- if doc.naming_series == \"Timber Products\" -%}\n                <td colspan=\"7\" style=\"text-align:right;\">\n                {%- else -%}\n                <td colspan=\"6\" style=\"text-align:right;\">\n                {%- endif -%}Loading Cost(Loading Rate x Total Qty):</td>\n                <td style = \"text-align : right;\">\n                    {{ doc.loading_rate }} x {{ doc.items|sum(attribute='qty') }} = {{ frappe.format_value(doc.loading_cost, {\"fieldtype\":\"Currency\"}) }}\n                </td>\n            </tr>\n            {%- endif -%}\n\n            <tr> \n                {%- if doc.naming_series == \"Timber Products\" -%}\n                <td colspan=\"7\" style='text-align:right'>\n                {%- else -%}\n                <td colspan=\"6\" style='text-align:right'>\n                {%- endif -%}\n                </td>\n                <!-- <b>Grand Amount</b></td>\n                <td style='text-align:right'>\n                    {{ frappe.format_value(doc.grand_total, {\"fieldtype\":\"Currency\"}) }}\n                </td> -->\n            </tr>\n        </table><br/>\n\n        <table>\n            <tr>\n                <td>\n                    <strong>Amount In Words:&nbsp</strong><i>{{  frappe.utils.money_in_words(doc.total) }}</i>\n                </td>\n                <td></td>\n                <td></td>\n                <td></td>\n                <td></td>\n                <td>\n                    <strong>Remarks: </strong><i>{{doc.remarks}}</i>\n                </td>\n            </tr>\n        </table>\n        <br/><br/><br/><br/>\n        <div class=\"row section-break\" style=\"margin-top: 60px;\">\n            <div class=\"col-xs-6 column-break\" style=\"text-align: left;\">\n                <strong>Authorized Signature: </strong> \n            </div>\n            <div class=\"col-xs-6 column-break\" style=\"text-align: left;\">\n                <strong>Signature: </strong><br><br>\n                <p><strong>Received by: </strong></p>\n            </div>\n        </div>\n    </div><br><br>\n</div> \n\n<div id=\"footer-html\" class=\"hidden-pdf\" style=\"position:relative; top: 130px; width: 100%\">\n        {% if not no_letterhead and footer %}\n            <div class=\"letter-head-footer\">\n            {{ footer }}\n            </div>\n        {% endif %}\n\n        <p class=\"text-center small page-number visible-pdf\">\n            {{ _(\"Page {0} of {1}\").format('<span class=\"page\"></span>', '<span class=\"topage\"></span>') }}\n        </p>\n</div>\n\n<div class=\"printedby\">\n    This print is system generated and should be considered VALID only with appropriate seal and/or signature. <br>  Printed by {{ frappe.user }} on {{ frappe.utils.nowdate()  }} at {{ frappe.utils.nowtime() }}\n</div>", 
