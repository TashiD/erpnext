 "css": "\n.table2, .table2 th, .table2 td {\n\tborder: 1px solid #999;\n        line-height: 1;   \n}\n.table {\n\twidth: 100%; \n}\n@media all {\n   div .printedby {\n       position: fixed;\n       bottom: 0;\n       text-align: right;\n       font-size: xx-small;\n       width: 100% !important;\n   }\n}", 
 "custom_format": 1, 
 "disabled": 0, 
 "doc_type": "Journal Entry", 
